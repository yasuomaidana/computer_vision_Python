    "    link = result.get_attribute(\"href\")\n",
    "    #Verifica que sea un link de synset\n",
    "    if (re.search(\"http://image-net.org/synset?\", link)):\n",
    "        if (len(links)):\n",
    "            #Si hay mas de un link, verifica que no sea igual al ultimo y lo guarda\n",
    "            if (link != links[-1]):\n",
    "                links.append(link)\n",
    "        else:\n",
    "            links.append(link)"
   ]
